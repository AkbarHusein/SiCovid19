{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "linebreak-style": "off",
    "import/no-extraneous-dependencies": "off",
    "import/extensions": "off",
    "global-require": "off",
    "no-underscore-dangle": "off",
    "comma-dangle": "off",
    "object-shorthand": "off",
    linebreak-style: "off",
  }
}
